{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "Client_URL": "[your cliente url]",
    "ReceivingEmail": "[email address where you wish to receive emails]"
  },
  "EmailSender": {
    "Host": "[example: smtp-mail.outlook.com]",
    "Port": 0,
    "EnableSSL": true,
    "UserName": "[sender email address]",
    "Password": "[sender email password]"
  },
  "AllowedHosts": "*"
}
